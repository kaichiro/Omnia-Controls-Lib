          <Comando>if PROTINA-&gt;notEmpty and (P_EXPO_ROT_TO.trim.Length &gt; 0) then
  PROTINA.ExportClass(P_EXPO_ROT_TO)-&gt;select( 'Rotina Exportada com sucesso!'.Msg )
