        </Atributos>
        <Relacionamentos />
      </Record>
      <Record Id="PRELATORIO" Tipo="RotinaParametro">
        <Atributos>
          <Comando>Relatorio.allLoadedObjects-&gt;select( r | r.Nome.sqlLikeCaseInsensitive('%{0}%') )-&gt;orderBy( r | r.Nome )</Comando>
