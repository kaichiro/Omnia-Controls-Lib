              </Atributos>
            </Record>
          </Campos>
        </Relacionamentos>
      </Record>
    </Parametros>
    <Comandos>
      <Record Id="" Tipo="RotinaComando">
        <Atributos>
          <Ativo>True</Ativo>
          <Comando>if PTME-&gt;notEmpty and (P_EXPO_TME_TO.trim.Length &gt; 0) then
  PTME.ExportClass(P_EXPO_TME_TO)-&gt;select( 'TipoMovimentoEstoque Exportado com sucesso!'.Msg )
else
  false
endif
</Comando>
          <Descricao>
          </Descricao>
          <Ordem>30</Ordem>
        </Atributos>
      </Record>
      <Record Id="" Tipo="RotinaComando">
        <Atributos>
          <Ativo>True</Ativo>
          <Comando>if PRELATORIO-&gt;notEmpty and (P_EXPO_REL_TO.trim.Length &gt; 0) then
  PRELATORIO.ExportClass(P_EXPO_REL_TO)-&gt;select( 'Relótorio Exportado com sucesso!'.Msg )
