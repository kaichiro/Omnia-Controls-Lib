        </Parameters>
        <SqlCommand>set dateformat ymd;

with infos010 as
(
	select 
	u.ECO_ID UnidadeId
	, u.Nome UnidadeDescricao
	, pp.Nr_Guia
	, case when fp.Fatura is not null then 1 else 0 end isGuiaFaturada
	from Protocolo p
	left join Unidade u on p.Unidade = u.ECO_ID
	inner join Protocolo_Procedimento pp on p.ECO_ID = pp.Protocolo
	left join Fatura_Procedimento fp on pp.ECO_ID = fp.Protocolo_Procedimento
	where p.Data_Entrada between @pDTIni and @pDTFim
	{vUnGlobal}
)
, infos020 as
(
	select distinct
	x.UnidadeId
	, x.UnidadeDescricao
	, x.Nr_Guia
	, x.isGuiaFaturada
	from infos010 x
)
, infos030 as
(
	select
	x.UnidadeDescricao
	, count(x.Nr_Guia) QtdeGuias
	, x.isGuiaFaturada
	from infos020 x
	group by
	x.UnidadeDescricao
	, x.isGuiaFaturada

)
	select
	1 flag
	, x.UnidadeDescricao
	, QtdeGuias
	, x.isGuiaFaturada
	, 'Quantidade de Guias Faturadas' + case x.isGuiaFaturada when 0 then ' não' else '' end + ' faturadas' StatusGuia
	from infos030 x</SqlCommand>
