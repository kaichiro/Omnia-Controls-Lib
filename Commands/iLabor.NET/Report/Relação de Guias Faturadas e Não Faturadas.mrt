        </Parameters>
        <SqlCommand>set dateformat ymd;

with infos010 as
(
	select 
	p.ECO_ID idProtocolo, u.Nome UnidadeDescricao
	from Protocolo p
	left join Unidade u on p.Unidade = u.ECO_ID
	where p.Data_Entrada between @pDTIni and @pDTFim
	{vUnGlobal}
)
, infos020 as
(
	select 
	x.idProtocolo, x.UnidadeDescricao, pp.ECO_ID idPP, pp.Nr_Guia
	from infos010 x
	inner join Protocolo_Procedimento pp on x.idProtocolo = pp.Protocolo
)
, infos030 as
(
	select
	x.idProtocolo, x.UnidadeDescricao, x.idPP, x.Nr_Guia, fp.ECO_ID idFP
	from infos020 x
	left join Fatura_Procedimento fp on x.idPP = fp.Protocolo_Procedimento
)
, infos040 as
(
	select
	x.idProtocolo, x.UnidadeDescricao, x.idPP, x.Nr_Guia, case when x.idFP is not null then 1 else 0 end isGuiaFaturada
	from infos030 x
)
, infos050 as
(
	select distinct
	x.UnidadeDescricao, x.Nr_Guia, x.isGuiaFaturada
	from infos040 x
)
	select 
	1 flag,
	x.UnidadeDescricao, COUNT(x.Nr_Guia) QtdeGuias, x.isGuiaFaturada,
	case x.isGuiaFaturada when 1 then 'Quantidade de Guias Faturadas' else 'Quantidade de Guias NÃO Faturadas' end StatusFaturamentoGuia
	from infos050 x
	group by
	x.UnidadeDescricao, x.isGuiaFaturada
	order by x.UnidadeDescricao</SqlCommand>
      </dsGeral02>
